! General settings
! -------------------------------------------------
URxvt.cursorBlink: true
URxvt.scrollBar: false
URxvt.borderLess: false

URxvt.inheritPixmap: false

URxvt.font: xft:Inconsolata:pixelsize=18
URxvt.fontBold: xft:Inconsolata:bold:pixelsize=18
URxvt.fontBoldItalic: xft:Inconsolata:bold:italic:pixelsize=18

! Color scheme
! -------------------------------------------------
URxvt.foreground: #FFFFFF
URxvt.background: [90]#4B5D7E

! Normal
! color0    black
! color1    red
! color2    green
! color3    yellow
! color4    blue
! color5    purple
! color6    cyan
! color7    white

urxvt.color0:    #6c6c6c
urxvt.color1:    #e9897c
urxvt.color2:    #b6e77d
urxvt.color3:    #ecebbe
urxvt.color4:    #a9cdeb
urxvt.color5:    #ea96eb
urxvt.color6:    #c9caec
urxvt.color7:    #CCCCCC

! Light
! color8     black
! color9     red
! color10    green
! color11    yellow
! color12    blue
! color13    purple
! color14    cyan
! color15    white

urxvt.color8:    #747474
urxvt.color9:    #f99286
urxvt.color10:    #c3f786
urxvt.color11:    #fcfbcc
urxvt.color12:    #b6defb
urxvt.color13:    #fba1fb
urxvt.color14:    #d7d9fc
urxvt.color15:    #e2e2e2


! Extensions
! -------------------------------------------------
URxvt.perl-ext-common: default,tabbed,fullscreen
URxvt.tabbed.tabbar-fg: 1 
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 4
URxvt.tabbed.tab-bg: 0
URxvt.keysym.F11: perl:fullscreen:switch
